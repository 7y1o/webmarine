    "@types/three": "^0.129.1",
    "@types/yuka": "^0.7.0",
    "babel-loader": "^8.2.2",
    "babel-preset-7-esnext": "^0.0.2",
    "chalk": "^2.4.2",
    "concurrently": "^6.2.0",
    "cypress": "^7.5.0",
    "karma-typescript": "^5.5.1",
    "terser-webpack-plugin": "^5.1.3",
    "typescript": "^4.3.2",

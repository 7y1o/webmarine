    "physics",
    "entity",
    "canvas",
    "webgl",
    "webgl2",
    "webgl1",
    "renderer",
    "ui",
    "interface",
    "play",
    "web",
    "nebula",
    "three",
    "cannon",
    "postprocess",
    "yuka"
  ],

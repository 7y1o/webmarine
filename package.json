{
  "name": "@7y1o/webmarine-core",
  "version": "0.0.3",
  "description": "Core utils for WebMarine game engine",
  "scripts": {
    "build": "npm run build:tsToJs && npm run build:jsToBundle",
    "build:tsToJs": "tsc",
    "build:jsToBundle": "webpack",
    "test": "concurrently -n server,cypress -c magenta,cyan \"npm run test:server\" \"npm run test:cypress\"",
    "test:server": "cd cypress/server && npm run dev",
    "test:cypress": "cypress open"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/magicians-game/webmarine-core.git"
  },
  "keywords": [
    "web",
    "marine",
    "webmarine",
    "utils",
